#Currency {
  align-items: center;
  flex-direction: row;
  height: 48px;
  left: 24px;
  position: absolute;
  top: 48px;
  border-radius: 24px;
}

#CurrencyIcon {
  -unity-background-image-tint-color: rgba(62, 42, 20, 1);
  height: 32px;
  width: 32px;
  flex-shrink: 0;
  margin: 0 8px;
}

#CurrencyText {
  -unity-text-align: middle-right;
  color: black;
  flex-grow: 1;
  text-align: right;
  font-size: 24px;
  margin: 0 8px;
}
